# Ignore configuration files that may contain sensitive information
/config/*.json
/config/*.yaml
/config/*.env

# Ignore node_modules directory (generated by npm/yarn)
/node_modules/

# Ignore log files
/logs/

# Ignore any compiled binaries or libraries
/bin/
/build/
/dist/

# Ignore any temporary files
/tmp/
.temp/

# Ignore environment-specific files
.env.local
.env.*.local
.env
# Ignore uploads (assuming it's user-generated content)
uploads

# Ignore any IDE or editor specific files
.vscode/
.idea/

# Ignore any backup files
*.bak
*.backup

# Ignore any package-lock.json or yarn.lock file
package-lock.json
yarn.lock
